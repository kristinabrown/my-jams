<h1 class="jumbotron text-center"><%= @song.title %><p>By <%= @song.artist %></p></h1>

<h2>Tags:<h2>
  <ul>
    <% @song.tags.each do |tag| %>
      <li><%= tag.name %></li>
    <% end %>
  </ul>

<%= link_to "Edit", edit_song_path(@song) %>

<%= link_to "Delete", song_path(@song), method: :delete %>
